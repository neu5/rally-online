{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["node", "express"],
    "typeRoots": ["babylonjs", "node_modules/ammojs-typed/ammo/ambient"]
  },
  "include": ["src/**/*.ts"]
}
